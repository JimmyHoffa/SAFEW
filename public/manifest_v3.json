
{
    "short_name": "SAFEW",
    "name": "Simple And Fast Ergo Wallet",
    "version": "0.1",
    "action": {},

    "background": {
        "service_worker": "./background.js"
    },
    "permissions": ["storage", "activeTab"],
    "host_permissions": [
        "<all_urls>"
      ],
    "manifest_version": 3,
    "web_accessible_resources": [{
      "resources": [
          "8652f0726f5673c6bedb.module.wasm",
          "index.html",
          "contentScript.js"
         ],
      "matches": ["*://*/*"],
      "extension_ids": []
    }],
    
    
     "content_security_policy": {
          "extension_pages": "script-src 'self' 'unsafe-eval' 'wasm-unsafe-eval'; object-src 'self'"
          },
     "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["contentScript.js"]
      }
    ]
  }
  
    